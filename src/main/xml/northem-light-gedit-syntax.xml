  <style name="secondary-cursor" use-style="cursor" />
  <style name="bracket-match" foreground="dark-gray" background="blue" />
  <style name="bracket-mismatch" foreground="red" underline="true" />
  <style name="search-match" foreground="snow" background="blue" underline="true" />
