  <color name="dark-gray"  value="#454546" />
  <color name="gray"       value="#6D6D6F" />
  <color name="light-gray" value="#C6C6C8" />
  <color name="dark-snow"  value="#F2F2F7" />
  <color name="dirty-snow" value="#F9F9FC" />
  <color name="snow"       value="#FFFFFF" />

  <color name="aqua"       value="#7ACAC2" />
  <color name="light-blue" value="#6FBAD0" />
  <color name="blue"       value="#71A7CA" />
  <color name="dark-blue"  value="#5781B3" />

